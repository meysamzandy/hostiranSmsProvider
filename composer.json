{
  "name": "meysam-znd/hostiran-sms-provider",
  "description": "A hostiran sms provider",
  "license": "MIT",
  "keywords": [
    "hostiran",
    "sms",
    "provider"
  ],
  "type": "library",
  "authors": [
    {
      "name": "meysam zandy",
      "email": "zandy.meysam@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0|^7.0",
    "ext-soap": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "MeysamZnd\\HostiranSmsProvider\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MeysamZnd\\HostiranSmsProvider\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MeysamZnd\\HostiranSmsProvider\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
